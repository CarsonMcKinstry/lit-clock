// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./quotes.db"
}

model Quote {
    id String @id @default(cuid())
    time String
    time_words String
    quote String
    title String
    author String
}

model Settings {
    id String @id
    dark_mode Boolean
}